{
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tap-chris/mamsc.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@tap-chris"
  },
  "name": "mamsc",
  "version": "0.1.6",
  "description": "MIDI Schow Control over Ethernet for MA lighting",
  "bugs": {
    "url": "https://github.com/tap-chris/mamsc/issues"
  },
  "keywords": [
    "mamsc",
    "ma",
    "lighting",
    "msc",
    "midi",
    "schow",
    "control",
    "ethernet"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "mamsc.js",
  "files": [
    "lib",
    "*.js",
    "*.hbs"
  ],
  "scripts": {
    "test": "standard lib/mamsc.es6.js",
    "docs": "jsdoc2md --template README.hbs --partial customTags.hbs --files lib/mamsc.es6.js > README.md",
    "build": "babel lib/mamsc.es6.js -o lib/mamsc.babel.js"
  },
  "author": "Christian Volmering <christian@volmering.name>",
  "license": "(MIT OR GPL-3.0)",
  "readmeFilename": "README.md",
  "dependencies": {
    "semver": "^5.7.1"
  },
  "optionalDependencies": {
    "@babel/runtime": "^7.7.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "jsdoc-to-markdown": "^5.0.3",
    "standard": "^14.3.1"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  }
}
